<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="view_progress_button">
        <attr name="displayText" format="string" localization="suggested" />
        <attr name="progressButtonTextColor" format="reference" />
        <attr name="progressBarColor" format="reference" />
        <attr name="progressButtonForegroundColor" format="reference" />
        <attr name="progressButtonStyle" format="enum">
            <enum name="filledBrand" value="0" />
            <enum name="filledAlternate" value="1" />
            <enum name="borderless" value="2" />
            <enum name="outline" value="3" />
        </attr>
    </declare-styleable>
    <declare-styleable name="progress_text_view">
        <attr name="text_view_display_text" format="string" localization="suggested" />
        <attr name="text_view_enabled_text_color" format="reference" />
        <attr name="text_view_disabled_text_color" format="reference" />
    </declare-styleable>
</resources>